<h3>New User</h3>

<%= form_with model: @user do |f| %>
  <%= f.label :username, 'Username:' %>
  <%= f.text_field :username %>
  <br>
  <%= f.label :email, 'Email:' %>
  <%= f.text_field :email, value: @user.email || 'example@example.com' %>
  <br>
  <%= f.label :password, 'Password:' %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit 'Submit' %>
<% end %>
